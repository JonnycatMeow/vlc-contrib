prefix=@@CONTRIB_PREFIX@@
exec_prefix=${prefix}
libdir=@@CONTRIB_PREFIX@@/lib
includedir=@@CONTRIB_PREFIX@@/include

Name: libass
Description: LibASS is an SSA/ASS subtitles rendering library
Version: 0.16.0
Cflags: -I${includedir}
Libs:  -L${libdir} -lass -liconv  -framework ApplicationServices -framework CoreFoundation  
Requires:  harfbuzz >= 1.2.3, fribidi >= 0.19.1, freetype2 >= 9.17.3  
